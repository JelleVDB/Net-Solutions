<Window x:Class="oefTeller.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:oefTeller"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnTel1" Content="Start tellen" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top" Width="75" Click="btnTel1_Click" />
        <Button x:Name="btnTel2" Content="Start tellen" HorizontalAlignment="Left" Margin="160,44,0,0" VerticalAlignment="Top" Width="75" Click="btnTel2_Click"/>
        <local:CustomTextVak x:Name="txtTel1" HorizontalAlignment="Left" Height="23" Margin="10,80,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="75"/>
        <local:CustomTextVak x:Name="txtTel2" HorizontalAlignment="Left" Height="23" Margin="160,80,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="75"/>
        <CheckBox x:Name="chkThreaded" Content="Threaded" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
